{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "extends": "expo/tsconfig.base",
  "include": ["env.d.ts", "src", "App.tsx", "**/*.ts", "**/*.tsx"]
}
